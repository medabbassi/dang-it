<p id="notice"><%= notice %></p>

<h1>User Regs</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_regs.each do |user_reg| %>
      <tr>
        <td><%= user_reg.username %></td>
        <td><%= user_reg.email %></td>
        <td><%= link_to 'Show', user_reg %></td>
        <td><%= link_to 'Edit', edit_user_reg_path(user_reg) %></td>
        <td><%= link_to 'Destroy', user_reg, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Reg', new_user_reg_path %>
